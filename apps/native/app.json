{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4C7AED",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.litecast"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "hidden": false
    },
    "assetBundlePatterns": ["**/*"],
    "backgroundColor": "#ffffff",
    "description": "A beautiful yet simple Farcaster client",
    "extra": {
      "eas": {
        "projectId": "8c7d3ef0-c9c4-4bd1-b71a-5d5cb32b20a7"
      }
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "bundleIdentifier": "com.litecast",
      "infoPlist": {
        "RCTAsyncStorageExcludeFromBackup": true,
        "UIBackgroundModes": ["remote-notification"]
      },
      "supportsTablet": true
    },
    "name": "Litecast",
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-router",
        {
          "asyncRoutes": "development",
          "origin": "<YOUR_WEB_URL>.vercel.app"
        }
      ]
    ],
    "primaryColor": "#ffffff",
    "privacy": "public",
    "scheme": "litecast",
    "slug": "litecast",
    "splash": {
      "backgroundColor": "#4C7AED",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    }
  }
}
